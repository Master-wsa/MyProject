package com.netctoss.entity;

import java.sql.Date;
/**
 * 月账单表
 * @author Administrator
 *
 */
public class Bill {
	private int bil_id; //月账单id
	private Account acc; //账务账号
	private Date bil_time; //年月
	private double bil_fee; //月费用
	private int bil_pway; //支付方式
	private int bil_pstate; //支付状态
	private int bil_alltime; //月时长
	public int getBil_id() {
		return bil_id;
	}
	public void setBil_id(int bil_id) {
		this.bil_id = bil_id;
	}
	public Account getAcc() {
		return acc;
	}
	public void setAcc(Account acc) {
		this.acc = acc;
	}
	public Date getBil_time() {
		return bil_time;
	}
	public void setBil_time(Date bil_time) {
		this.bil_time = bil_time;
	}
	public double getBil_fee() {
		return bil_fee;
	}
	public void setBil_fee(double bil_fee) {
		this.bil_fee = bil_fee;
	}
	public int getBil_pway() {
		return bil_pway;
	}
	public void setBil_pway(int bil_pway) {
		this.bil_pway = bil_pway;
	}
	public int getBil_pstate() {
		return bil_pstate;
	}
	public void setBil_pstate(int bil_pstate) {
		this.bil_pstate = bil_pstate;
	}
	public int getBil_alltime() {
		return bil_alltime;
	}
	public void setBil_alltime(int bil_alltime) {
		this.bil_alltime = bil_alltime;
	}
}
